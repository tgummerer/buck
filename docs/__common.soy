<head prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns#">
<script>
  var loc = new String(window.location);
  if (loc.indexOf('facebook.github.io/buck') != -1) {lb}
    window.location.replace(loc.replace('facebook.github.io/buck', 'buckbuild.com'));
